{
  "compilerOptions": {
    "strict": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "target": "ES2015",
    "noFallthroughCasesInSwitch": true,
    "exactOptionalPropertyTypes": true,
  },
  "include": [
    "public/**/*.js",
  ]
}